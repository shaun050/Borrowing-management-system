<div class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Employee</h1>
    <div class="btn-toolbar mb-2 mb-md 0">
      <div class="btn-group me-2">
        <button class="btn btn-sm btn-outline-secondary">?</button>
      </div>
    </div>
  </div>

  <div class="col py-3">
    <div class="row">
      <div class="col">
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
            <li class="breadcrumb-item active">New Employee</li>
          </ol>
        </nav>
      </div>
      <div class="col text-end fw-lighter">
        <b>UserId</b>
      </div>
    </div>
  
  </div>

  <form method="POST" action="/register">
   
  

    <div class="row form-group mb-4">
      <div class="col">
        <label for="employeeName">Employee Name</label>
        <input type="text" class="form-control" id="employeeName" name="employeeName" value="" placeholder="Employee Name" required>
      </div>
  
      <div class="col">
        <label for="employeeId">Employee ID</label>
        <input type="number" class="form-control" id="employeeId" name="employeeId" value="" placeholder="Employee ID" required>
      </div>
    </div>
  
    <div class="row form-group mb-4">
      <div class="col">
        <label for="employeeEmail">Employee Email</label>
        <input type="email" class="form-control" id="employeeEmail" name="employeeEmail" value="" placeholder="Employee Email" required>
      </div>
  
      
    </div>
  
  
    
  
    <div class="form-group mb-4">
      <button type="submit" class="btn btn-primary">Add Employee</button>
    </div>
  
    
  
  </form>